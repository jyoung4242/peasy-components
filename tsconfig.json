{
    "compilerOptions": {
        "outDir": "./build/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "experimentalDecorators": true
    }
}
